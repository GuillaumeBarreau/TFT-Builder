{
  "editor.tabSize": 2,
  // Run ESLint each time you save a file
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  // Disable Prettier for JavaScript and React (but not for HTML, CSS or others in future)
  "prettier.disableLanguages": [
    "javascript",
    "javascriptreact"
  ],
}